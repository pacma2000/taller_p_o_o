package punto6;

class punto6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int longitud = 4;
		int cuadrado;
		int area;
		
		cuadrado = longitud * longitud;
		
		area =cuadrado;
		
		System.out.println("El area del cuadrado es igual a "+area+"cm^2");
		
		

	}

}
